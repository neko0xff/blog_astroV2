    "outdated_update": "deno outdated --update --latest",
    "bench": "deno bench -A --unstable-kv --unstable-ffi  bench/*",
    "install": "deno install --allow-scripts ",
    "test": "deno test --allow-net",
    "lint": "deno lint --fix"
  },
  "deploy": {
    "project": "495ce49b-e4ae-4188-91d3-8a3bf56a2e90",
